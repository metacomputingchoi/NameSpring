{
  "separator_length": 100,
  "title": "상세 성명학 분석 보고서",
  "header_format": "【 {name}({hanja}) {title} 】",
  "separators": {
    "main": "=",
    "section": "─",
    "subsection": "-"
  },
  "section_separator_length": 50,
  "prefixes": {
    "section": "◆",
    "subsection": "▸",
    "item": "•",
    "subitem": "◦",
    "tree": "└"
  },
  "info_formats": {
    "name": "{prefix} {label}: {fullName} ({fullHanja})",
    "score": "{prefix} {label}: {score}점 / 100점",
    "grade": "{prefix} {label}: {grade}등급"
  },
  "progress_bar": {
    "length": 20,
    "unit": 5,
    "filled": "█",
    "empty": "□",
    "format": "  {prefix} {category}: {current}/{max} 점 [{bar}] {percentage}%"
  },
  "dot_display": {
    "max": 8,
    "filled": "●",
    "empty": "○",
    "format": "  {prefix} {element}: {bar} ({count})"
  },
  "saju_formats": {
    "pillar": "  {prefix} {name}: {interpretation}",
    "analysis": "    {line}"
  },
  "stroke_formats": {
    "number": "  {prefix} {name}: {stroke} 획",
    "luck": "  {prefix} {name}:",
    "meaning": "  {prefix} {name}: {meaning}"
  },
  "element_formats": {
    "relation": "  {prefix} {from} → {to}: {type}",
    "influence": "    {prefix} {influence}",
    "strength": "  {prefix} {position}: {strength}"
  },
  "yinyang_formats": {
    "distribution": "{prefix} {label}: 음({yin}) : 양({yang})",
    "score": "{prefix} {label}: {score}/10"
  },
  "character_formats": {
    "hanja": "\n{prefix} {character} ({meaning}):",
    "origin": "  {prefix} 어원: {origin}",
    "components": "  {prefix} 구성: {components}",
    "related": "  {prefix} 관련자: {related}"
  },
  "list_formats": {
    "item": "  {prefix} {item}",
    "joined": ", "
  },
  "footer_disclaimer_prefix": "※ ",
  "magic_numbers": {
    "max_score": 100,
    "balance_max": 10,
    "section_width": 50,
    "header_width": 100
  }
}